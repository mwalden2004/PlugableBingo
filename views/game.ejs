    <link rel="stylesheet" href="/assets/css.css">

    <title><%= game.title %> Bingo!</title>
    <script>
        const game = <%- JSON.stringify(game) %>;
    </script>
  </head>
  <body>
    
    <div id="won" class="hidden">
        <h1 id="msg">Passive Agressive Win Message</h1>
